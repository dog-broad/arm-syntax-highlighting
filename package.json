{
  "name": "arm-syntax-highlighting",
  "displayName": "ARM Syntax Highlighting",
  "description": "This Visual Studio Code extension provides syntax highlighting for ARM Assembly language, based on the original Notepad++ ARM Language Highlighting by BluRose.",
  "version": "0.0.1",
  "publisher": "Dog-broad",
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "language": "arm-asm",
        "scopeName": "source.arm-asm",
        "path": "./syntaxes/arm-asm.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "arm-asm",
        "aliases": ["ARM ASM", "arm-asm"],
        "extensions": [".S", ".s"],
        "configuration": "./language-configuration.json"
      }
    ],
    "themes": [
      {
        "label": "ARM ASM Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/arm-asm-dark-color-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
